<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ST.Carputer.Pages.NavigationPage">
    <ContentPage.Content>
        <StackLayout Margin="0,20,0,0">
            <Label Text="{Binding Latitude}" TextColor="White"></Label>
            <Label Text="{Binding Longitude}" TextColor="White"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>